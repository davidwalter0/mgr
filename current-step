WRK_STEP_START=${WRK_STEP_START:=${WRK_STATUS_PATH}/${WRK_STEP}.start}
WRK_STEP_DONE=${WRK_STEP_DONE:=${WRK_STATUS_PATH}/${WRK_STEP}.done}
WRK_STEP_ERROR=${WRK_STEP_ERROR:=${WRK_STATUS_PATH}/${WRK_STEP}.error}
printf "$(now) WRK_STEP_START=${WRK_STEP_START}\n"
printf "$(now) WRK_STEP_DONE=${WRK_STEP_DONE}\n"
printf "$(now) WRK_STEP_ERROR=${WRK_STEP_ERROR}\n"
